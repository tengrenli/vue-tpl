    "vue": "^2.6.10",
    "vue-router": "^3.1.3"{{#if vuex}},
    "vuex": "^3.1.2"
    {{/if}}
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
